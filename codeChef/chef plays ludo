#include <bits/stdc++.h>
using namespace std;

int main() {
	int t; cin >> t;
	while (t--){
	    int x ; cin >> x;
	    if (x==6){
	        cout << "YES" << endl;
	    }
	    else {
	        cout << "NO" << endl;
	    }
	}

}
